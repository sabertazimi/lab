{
  "name": "@lab/m-league-reviewer",
  "type": "module",
  "version": "3.2.0",
  "private": true,
  "packageManager": "pnpm@10.30.1",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "start": "react-router-serve ./build/server/index.js",
    "lint": "eslint . && pnpm lint:typecheck",
    "lint:typecheck": "react-router typegen && tsc",
    "format": "git diff --name-only --cached --relative | grep -E \"\\.(ts|tsx|js|jsx)$\" | xargs -r prettier --write && git diff --name-only --cached --relative | grep -E \"\\.(ts|tsx|js|jsx)$\" | xargs -r eslint --fix",
    "test": "vitest run --coverage",
    "fetch": "tsc --project tsconfig.scripts.json && node scripts/fetch.js && node scripts/fix.js",
    "fetch:game-schedule": "tsc --project tsconfig.scripts.json && node scripts/fetch-game-schedule.js"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^13.11.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-router/node": "^7.13.0",
    "@react-router/serve": "^7.13.0",
    "@tiptap/extensions": "^3.20.0",
    "@tiptap/pm": "^3.20.0",
    "@tiptap/react": "^3.20.0",
    "@tiptap/starter-kit": "^3.20.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "isbot": "^5.1.35",
    "lucide-react": "^0.575.0",
    "motion": "^12.34.3",
    "next-themes": "^0.4.6",
    "react": "^19.2.4",
    "react-day-picker": "^9.13.2",
    "react-dom": "^19.2.4",
    "react-router": "^7.13.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.5.0",
    "zustand": "^5.0.11"
